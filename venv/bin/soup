#!/home/alpesh/PycharmProjects/btp1/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pysoup==0.1.2','console_scripts','soup'
__requires__ = 'pysoup==0.1.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pysoup==0.1.2', 'console_scripts', 'soup')()
    )
